{
    "name": "rawphp/raw-database",
    "type": "library",
    "description": "RawDatabase is a wrapper with useful utility functions around the MySQLi class and is used by RawPHP framework and other applications.",
    "keywords": [ "database", "mysql", "mysqli" ],
    "homepage": "http://rawphp.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Kaczocha",
            "email": "tom@rawphp.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "rawphp/raw-support": "0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.5",
        "rawphp/raw-code-standards": "1.*@dev",
		"satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "RawPHP\\RawDatabase\\": "src/RawPHP/RawDatabase/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
